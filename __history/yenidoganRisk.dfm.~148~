      FieldName = 'BOY_CM'
    end
    object fltfldHastaBAS_CEVRESI_CM: TFloatField
      FieldName = 'BAS_CEVRESI_CM'
    end
  end
  object dsHasta: TDataSource
    DataSet = rastgeleHasta
    Left = 896
    Top = 16
  end
end
