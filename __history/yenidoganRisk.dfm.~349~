  object qryMaxHafta: TOraQuery
    Session = orsn1
    SQL.Strings = (
      'SELECT'
      '  NVL(MAX(FORM_NO), 0) AS FORM_NO,'
      '  COUNT(*) AS HAFTA_SAYISI'
      'FROM HASTANE.YD_RISK_IZLEM'
      'WHERE DOSYA_NO = :DOSYA_NO'
      '  AND PROTOKOL_NO = :PROTOKOL_NO'
      '  AND FORM_NO = ('
      '    SELECT NVL(MAX(FORM_NO), 0)'
      '    FROM HASTANE.YD_RISK_IZLEM'
      '    WHERE DOSYA_NO = :DOSYA_NO'
      '      AND PROTOKOL_NO = :PROTOKOL_NO'
      '  )')
    Left = 1003
    Top = 8
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DOSYA_NO'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'PROTOKOL_NO'
        Value = nil
      end>
